{
  "name": "blue-fig-editions",
  "version": "1.0.0",
  "description": "A gallary/e-commerce site.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "seed": "node scripts/seedDB.js",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkennedy118/blue-fig-editions.git"
  },
  "author": "Tyler Kennedy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tkennedy118/blue-fig-editions/issues"
  },
  "homepage": "https://github.com/tkennedy118/blue-fig-editions#readme",
  "dependencies": {
    "@easypost/api": "^3.9.1",
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.9.27",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "stripe": "^8.96.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
